#include "ap.h"

void delay(uint32_t ms);
uint32_t millis(void);

void apInit(void)
{
  //
}

void apMain(void)
{
  while(1)
  {
    void HAL_GPIO_TogglePin(GPIOC, GPIO_PIN_13);
    delay(1000);
  }
}

void delay(uint32_t ms)
{
  //
  HAL_Delay(ms);
}

uint32_t millis(void)
{

  return 0;
}
